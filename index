<script>var counting_time = 0;
    
var startTime, interval,loaded_time;

function start(){
    startTime = Date.now();
    interval = setInterval(function(){
        updateDisplay(Date.now() - startTime);
    });
 
}
   function updateDisplay(currentTime){
    counting_time =currentTime;
}
start();

</script>
