	<script>
            var timerRandom;

            function getRandomIntBetween(min, max) {
                min = Math.ceil(min);
                max = Math.floor(max);
                return Math.floor(Math.random() * (max - min + 1)) + min;
            }

            function countDown(i, callback) {
          
                timerRandom = setInterval(function() {
                    document.getElementById("getpassCode").innerHTML = "ƒêang T·∫£i. Vui L√≤ng ƒê·ª£i "+i+" Gi√¢y";
                    i-- || (clearInterval(timerRandom), callback());
                }, 1000);
            }


            jQuery("#getpassCode").one("click", function(){
       
               const randomMonth = getRandomIntBetween(30, 86);
                countDown(randomMonth , function(){
                    document.getElementById("getpassCode").innerHTML = '33867892022';
                });
            });
        </script>


</footer>

