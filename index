        userId: 0,
        canUseChat: false,
        serverTime: 1602227951,
        channel: 'room',
        roomId: 1,
        lastId: { "1": 5543671 },
        convId: 0,
        convUnread: [],
        convOnly: 0,
        convItems: [],
        guestRoom: 1,
        messageDisplayLimit: 25,
        notificationTimeout: 5000,
        refreshActive: 5000,
        refreshActiveHidden: 8000,
        refreshInactive: 20000,
